{
  "name": "gen",
  "version": "1.0.0",
  "main": "dist/api.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/api.js",
    "dev2": "concurrently \"yarn build -w\" \"nodemon dist/api.js\"",
    "dev": "ts-node-dev --respawn --transpileOnly ./api.ts",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "yarn clean && yarn build",
    "clean": "rimraf dist",
    "predev": "yarn clean && yarn build"
  },
  "dependencies": {
    "express": "^4.17.3",
    "mongodb": "^3.2.3",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/mongodb": "^3.1.24",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.13.0",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10",
    "rimraf": "^2.6.3",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.4.1"
  }
}
